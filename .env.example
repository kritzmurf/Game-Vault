# Template for local environment variables.
# Copy this file to .env and fill in real values.
# .env is gitignored and never committed.

# PostgreSQL
DB_USER=postgres
DB_PASSWORD=your_password_here
DB_NAME=gamevault
DB_PORT=5432

# Backend
ConnectionStrings__DefaultConnection="Host=localhost;Port=5432;Database=gamevault;Username=postgres;Password=your_password_here"

# pgAdmin
PGADMIN_EMAIL=admin@example.com
PGADMIN_PASSWORD=your_password_here
PGADMIN_PORT=5050
